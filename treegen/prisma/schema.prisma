generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Story {
  id       String  @id @default(uuid())
  cname    String? @unique
  duration Int?
  asmId    String?
  title    String?

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  // blobs
  meta      Json?
  chapters  Json?
  chunks    Json?
  entities  Json?
}
